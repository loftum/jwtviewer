             xmlns:views="clr-namespace:JwtViewer.Views"
             x:Class="JwtViewer.App"
             Name="Jwt viewer">
    <Application.DataTemplates>
        <views:ViewLocator/>
    </Application.DataTemplates>
    
    <Application.Styles>
